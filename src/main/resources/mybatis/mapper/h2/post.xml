<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop.onekorea.springboot.mapper.PostMapper">
        <select id="listPost" resultType="shop.onekorea.springboot.entity.Post">
            select
                post_id,
                title,
                contents,
                author,
                created_at
            from post
        </select>
</mapper>